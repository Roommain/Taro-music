
.list {
  position: relative;
  width: 100%;
  height: 120rpx;
  margin: 15rpx 0;
  border-bottom: 1rpx solid rgba(197, 193, 193, 0.5);
}
.singerImg {
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
}
.clearfix:after {
  content: ".";
  display: block;
  height: 0;
  clear: both;
  visibility: hidden;
}
.left {
  position: absolute;
  left: 20rpx;
}
.right {
  position: absolute;
  left: 150rpx;
  font-size: 26rpx;
}
